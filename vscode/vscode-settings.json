{
  "zenMode.fullScreen": false,
  "workbench.sideBar.location": "right",
  "workbench.iconTheme": "dark-minimalist-icons",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.activityBar.location": "top",
  "workbench.layoutControl.enabled": false,
  "extensions.ignoreRecommendations": true,
  "editor.formatOnSave": true,
  "editor.codeLensFontFamily": "CommitMono",
  "editor.fontFamily": "CommitMono, Fira Code, Menlo, Monaco, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "eslint.codeActionsOnSave.rules": null,
  "editor.guides.bracketPairs": true,
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.unicodeHighlight.ambiguousCharacters": true,"svg.preview.mode": "svg",
  "editor.stickyScroll.enabled": true,
  "testing.openTesting": "neverOpen",
  "terminal.external.osxExec": "Warp.app",
  "gitlens.plusFeatures.enabled": false,
  "nxConsole.showNodeVersionOnStartup": false,
  "jest.runMode": "on-demand",
  "jestrunner.codeLens": [],
  "jestrunner.disableCodeLens": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.formatOnSave": false
  }
}
